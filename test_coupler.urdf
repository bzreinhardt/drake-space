	<inertial>
	    <mass value="1"/>
	    <origin xyz="0 0 0"/>
	    <inertia ixx="1" ixy="0" ixz="0" iyx="0" iyy="1" iyz="0" izx="0" izy="0" izz="1" />
	</inertial>
	<visual>
	    <origin rpy="0 0 0" xyz="0 0 0" />
	    <geometry>
		<box size="0.001 0.001 0.001" />
	    </geometry>
	</visual>
	<collision>
	    <origin rpy="0 0 0" xyz="0 0 0" />
